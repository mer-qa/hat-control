[Common]
Samplerate=10
Samples=10

[Sensor1]
Type=Current
Function= #Optic_get_color
Samplerate=10
String0=mA
String1=
Dout=1 # 0 = low range 0-100mA, 1 = high range 0-1500mA
Datafile=
Offset=-0
Multiplier0=1
Multiplier1=1

